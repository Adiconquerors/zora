	"require": {
		"laravel/framework": "^8.0"
	},
	"license": "MIT",
	"authors": [
		{
			"name": "secondman",
			"email": "secondman@jetstreamlabs.com"
		}
	],
	"autoload": {
		"psr-4": {
			"Serenity\\Zora\\": "src/"
		}
	},
	"extra": {
		"laravel": {
			"providers": [
				"Serenity\\Zora\\ZoraServiceProvider"
			]
		}
	},
	"require-dev": {
		"styleci/cli": "^1.2"
	}
}
